	date = {2022-03-09},
	langid = {english},
	note = {74 citations (Crossref/{DOI}) [2025-05-15]
Publisher: Public Library of Science},
	keywords = {Citation analysis, Gene expression, Genome analysis, Genome annotation, Genomics, Glucose, Microarrays, Software tools},
	file = {Full Text PDF:/Users/loganlarlham/Zotero/storage/V76E3NGT/Wijesooriya et al. - 2022 - Urgent need for consistent standards in functional enrichment analysis.pdf:application/pdf},
