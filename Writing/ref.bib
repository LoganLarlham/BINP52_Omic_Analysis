	note = {97 citations (Crossref/{DOI}) [2025-05-15]
Publisher: American Physiological Society},
	keywords = {microglia, acute spinal cord injury, {lncRNA} {MALAT}1, {miR}-199b},
	file = {Full Text PDF:/Users/loganlarlham/Zotero/storage/CLHWBY5E/Zhou et al. - 2018 - Long noncoding RNA MALAT1 contributes to inflammatory response of microglia following spinal cord in.pdf:application/pdf},
