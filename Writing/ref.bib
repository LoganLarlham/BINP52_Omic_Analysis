	keywords = {Alzheimer's disease, Neuroimmunology, {CD}45, complement receptor 3, dexamethasone, Fcγ receptor, macrosialin ({CD}68), Mechanism of Action, Microglial Cells, Neurology, Neuropharmacology, scavenger receptor class A},
	file = {Full Text PDF:/Users/loganlarlham/Zotero/storage/JHQSTTYS/Herber et al. - 2007 - Microglial Activation is Required for Aβ Clearance After Intracranial Injection of Lipopolysaccharid.pdf:application/pdf},
