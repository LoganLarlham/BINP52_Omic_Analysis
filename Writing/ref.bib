	urldate = {2025-04-16},
	date = {1989-10-01},
	note = {737 citations (Crossref/{DOI}) [2025-05-15]},
	keywords = {Alzheimer's disease, Amyloid protein, Human leukocyte antigen ({HLA})-{DR}-positive reactive microglia, Reactive astrocyte, Senile plaque},
