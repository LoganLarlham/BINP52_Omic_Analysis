@article{li_long_2024,
	title = {Long Non-Coding {RNA} {MALAT}1 Protects Against Spinal Cord Injury via Suppressing {microRNA}-125b-5p Mediated Microglial M1 Polarization, Neuroinflammation, and Neural Apoptosis},
