	urldate = {2025-04-16},
	date = {2016-03-22},
	note = {1498 citations (Crossref/{DOI}) [2025-05-15]
Publisher: Proceedings of the National Academy of Sciences},
	file = {Full Text PDF:/Users/loganlarlham/Zotero/storage/IABKKDV4/Bennett et al. - 2016 - New tools for studying microglia in the mouse and human CNS.pdf:application/pdf},
