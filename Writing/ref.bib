	urldate = {2024-04-26},
	date = {2023-03-28},
	note = {92 citations (Crossref/{DOI}) [2025-05-15]},
	keywords = {microglia, Apolipoprotein E, {APOE}, aging, amyloid, {DAM}, immunometabolism, {LPS}, {scRNA}-seq, spatial transcriptomics},
