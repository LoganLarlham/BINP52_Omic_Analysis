	pmcid = {PMC10585767},
	note = {10 citations (Crossref/{DOI}) [2025-05-15]},
	keywords = {Alzheimer’s disease, Apolipoprotein E, Alzheimer Disease, Female, Male, Neuroinflammation, Animals, Mice, Amyloid beta-Peptides, Apolipoprotein E3, Apolipoprotein E4, Apolipoproteins E, Cytokines, {EFAD}, Female sex, {IAXO}-101, Mice, Transgenic, Neuroinflammatory Diseases, {TLR}4, Toll-Like Receptor 4, Treatment paradigms},
