	urldate = {2025-02-14},
	date = {2024-05},
	langid = {english},
	note = {48 citations (Crossref/{DOI}) [2025-05-15]
Publisher: Nature Publishing Group},
	keywords = {Cellular neuroscience, Alzheimer's disease, Microglia},
	file = {Full Text PDF:/Users/loganlarlham/Zotero/storage/IYNGNTIY/Mancuso et al. - 2024 - Xenografted human microglia display diverse transcriptomic states in response to Alzheimer’s disease.pdf:application/pdf},
}

